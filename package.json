{
  "name": "help-me-chat",
  "displayName": "Help Me Chat",
  "description": "Resolver errors that appear in the terminal",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "help-me-chat.error",
        "title": "Error Helper"
      }
    ]
  },
  "scripts": {
    "start": "set NODE_ENV=development",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "openai": "^3.2.1",
    "showdown": "^2.1.0"
  }
}
